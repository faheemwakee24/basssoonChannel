{
  "extends": "@react-native/typescript-config",
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "src/types/**/*.d.ts"
  ],
  "exclude": [
    "**/node_modules",
    "**/Pods"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@": [
        "src"
      ],
      "@/components/*": [
        "src/components/*"
      ],
      "@/api/*": [
        "src/api/*"
      ],
      "@/assets/*": [
        "src/assets/*"
      ],
      "@/config/*": [
        "src/config/*"
      ],
      "@/hooks/*": [
        "src/hooks/*"
      ],
      "@/navigation/*": [
        "src/navigation/*"
      ],
      "@/screens/*": [
        "src/screens/*"
      ],
      "@/store/*": [
        "src/store/*"
      ],
      "@/types/*": [
        "src/types/*"
      ],
      "@/utils/*": [
        "src/utils/*"
      ]
    },
    "typeRoots": [
      "./src/types",
      "./node_modules/@types"
    ]
  }
}